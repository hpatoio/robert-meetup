{
  "name": "meetup-api",
  "version": "0.1.0",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "rm -rf build/ && tsc",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepare": "husky install",
    "consumer": "NODE_OPTIONS=--openssl-legacy-provider ts-node src/consumer.ts",
    "smart-contract:deploy": "NODE_OPTIONS=--openssl-legacy-provider ts-node scripts/ganacheDeployContracts.ts",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch"
  },
  "dependencies": {
    "@truffle/hdwallet-provider": "^2.0.8",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/jest": "^28.1.1",
    "arweave": "^1.10.23",
    "axios": "^0.27.2",
    "defender-relay-client": "^1.21.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-validator": "^6.14.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.16",
    "throng": "^5.0.0",
    "ts-node": "^10.7.0",
    "web3": "^1.7.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "prettier": "2.6.2",
    "ts-jest": "^28.0.4"
  }
}
